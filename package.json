{
  "name": "leapfrog",
  "version": "1.0.0",
  "description": "GUI for AWS DynamoDB",
  "main": "public/electron.js",
  "scripts": {
    "electron": "electron .",
    "create": "webpack",
    "dev": "webpack-dev-server --open",
    "pack": "build --dir",
    "dist": "build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "build -mw"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itzsrikanth/LeapFrog.git"
  },
  "keywords": [
    "aws",
    "dynamodb",
    "electron"
  ],
  "author": "Srikanth Sharma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itzsrikanth/LeapFrog/issues"
  },
  "homepage": "./",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.2",
    "electron-builder": "^20.43.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.5.1",
    "wait-on": "^3.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
